@model IEnumerable<CMSProject.Models.OrderReport>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/admin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CustomerID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Interest)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Interest)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.OrderReportID }) |
                @Html.ActionLink("Details", "Details", new { id = item.OrderReportID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.OrderReportID })
            </td>
        </tr>
    }

</table>
