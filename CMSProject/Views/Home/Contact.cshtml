    <section class="contact-section padding_top">
        <div class="container">        
            <img src="~/Content/Uploads/lienhe.jpg" width="2000" height="400" />
            <div class="row">
                <div class="col-12">
                    <h2 class="contact-title">Quý khách vui lòng điền thông tin theo mẫu form dưới đây để liên hệ với chúng tôi:</h2>
                </div>
                <div class="col-lg-8">
                    <form class="form-contact contact_form" action="contact_process.php" method="post" id="contactForm"
                          novalidate="novalidate">
                        <div class="row">
                            <div class="col-12">
                                <div class="form-group">
                                    <textarea class="form-control w-100" name="message" id="message" cols="30" rows="9"
                                              onfocus="this.placeholder = ''" onblur="this.placeholder = 'Yêu cầu'"
                                              placeholder='Điền yêu cầu của quý khách'></textarea>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group">
                                    <input class="form-control" name="name" id="name" type="text" onfocus="this.placeholder = ''"
                                           onblur="this.placeholder = 'Họ và tên'" placeholder='Họ và tên'>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group">
                                    <input class="form-control" name="email" id="email" type="email" onfocus="this.placeholder = ''"
                                           onblur="this.placeholder = 'Địa chỉ'" placeholder='Địa chỉ '>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="form-group">
                                    <input class="form-control" name="subject" id="subject" type="text" onfocus="this.placeholder = ''"
                                           onblur="this.placeholder = 'Yêu cầu'" placeholder='Yêu cầu'>
                                </div>
                            </div>
                        </div>
                        <div class="form-group mt-3">
                            <a href="#" class="btn_3 button-contactForm">Gửi tin nhắn</a>
                        </div>
                    </form>
                </div>
                <div class="col-lg-4">
                    <div class="media contact-info">
                        <span class="contact-info__icon"><i class="ti-home"></i></span>
                        <div class="media-body">
                            <h3>Xóm Cầm Lũy , Xã Cẩm La , Tx.Quảng Yên-Quảng Ninh</h3>
                            <p>Địa chỉ </p>
                        </div>
                    </div>
                    <div class="media contact-info">
                        <span class="contact-info__icon"><i class="ti-tablet"></i></span>
                        <div class="media-body">
                            <h3>0987237158</h3>
                            <p>Mở cửa từ 7h AM- 9h PM</p>
                        </div>
                    </div>
                    <div class="media contact-info">
                        <span class="contact-info__icon"><i class="ti-email"></i></span>
                        <div class="media-body">
                            <h3>support@CMS.com</h3>
                            <p>Email liên hệ!</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
