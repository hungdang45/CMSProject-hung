@model IEnumerable<CMSProject.Models.Feedback>

@{
    
    Layout = "~/Views/Shared/admin.cshtml";
}
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FeedbackContent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Customer.CustomerName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FeedbackContent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Customer.CustomerName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.FeedbackID }) |
                @Html.ActionLink("Details", "Details", new { id = item.FeedbackID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.FeedbackID })
            </td>
        </tr>
    }

</table>
